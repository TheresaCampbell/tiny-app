<!-- Header that shows a welcome message and logout button if user is logged in, or login and register links if user is logged out. -->
<h1>My URL Shortener</h1>
<table>
<% if (user) { %>
  <tr>
    <td>
      <form method="POST" action="/logout">
        <input type="Submit" value="Logout"/>
    </td>
    <td>
      <h5>Welcome <%= user.email %></h5>
    </td>
      </form>
  </tr>
  <% } else { %>
    <tr>
      <td>
        <a href="/login">Log In</a>
      </td>
      <td>
        <a href="/register">Register</a>
      </td>
      </form>
    <% } %>
  </tr>
</table>

