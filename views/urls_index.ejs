<!DOCTYPE html>
<html lang="en">
  <head>
    <title>TinyApp</title>
  </head>
  <body>
    <header>
    <%- include partials/_header.ejs %>
    </header>
    <table class="table">
      <thead>
        <tr>
          <th>Short URLs</th>
          <th>Long URLs</th>
          <th>userID of Creator</th>
        </tr>
      </thead>
      <tbody>
        <% urls.forEach(url => { %>
        <tr>
          <td><%= url.shortURL %></td>
          <td><%= url.longURL %></td>
         <!--  Added userID of creator. Maybe didn't have to... -->
          <td><%= url.userID %>
          <td>
            <a href="/urls/<%=url.shortURL%>">Edit</a>
          </td>
          <td>
            <form method="POST" action="/urls/<%=url.shortURL%>/delete">
              <input type="Submit" value="Delete"/>
            </form>
          </td>
        </tr>
        <% }) %>
        <tr>
          <td colSpan="2">
            <a href="/urls/new">New Tiny URL</a>
          </td>
        </tr>
      </tbody>
    </table>
  </body>
</html>